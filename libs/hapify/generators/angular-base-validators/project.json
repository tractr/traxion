{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "name": "hapify-generators-angular-base-validators",
  "projectType": "library",
  "sourceRoot": "libs/hapify/generators/angular-base-validators/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "assets": ["libs/hapify/generators/angular-base-validators/*.md"],
        "main": "libs/hapify/generators/angular-base-validators/src/index.ts",
        "outputPath": "dist/libs/hapify/generators/angular-base-validators",
        "packageJson": "libs/hapify/generators/angular-base-validators/package.json",
        "tsConfig": "libs/hapify/generators/angular-base-validators/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/hapify/generators/angular-base-validators/**/*.ts"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/hapify/generators/angular-base-validators/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/hapify/generators/angular-base-validators"]
    },
    "ts-morph-generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx ts-node -r tsconfig-paths/register --project ./tsconfig.lib.json ./src/start.ts",
            "forwardAllArgs": false
          }
        ],
        "cwd": "libs/hapify/generators/angular-base-validators",
        "parallel": false
      }
    }

  }
}
