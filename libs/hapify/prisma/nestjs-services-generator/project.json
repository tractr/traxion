{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "name": "hapify-prisma-nestjs-services-generator",
  "projectType": "library",
  "sourceRoot": "libs/hapify/prisma/nestjs-services-generator/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "options": {
        "assets": ["libs/hapify/prisma/nestjs-services-generator/*.md"],
        "main": "libs/hapify/prisma/nestjs-services-generator/src/index.ts",
        "outputPath": "dist/libs/hapify/prisma/nestjs-services-generator",
        "tsConfig": "libs/hapify/prisma/nestjs-services-generator/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/hapify/prisma/nestjs-services-generator/**/*.ts"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/hapify/prisma/nestjs-services-generator/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    },
    "update-peer-dependencies": {
      "executor": "./dist/libs/schematics:update-peer-dependencies",
      "options": {
        "packageJsonPath": "{workspaceRoot}/dist/libs/hapify/prisma/nestjs-services-generator/package.json"
      }
    }
  }
}
