{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/nestjs/winston/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "assets": [
          "libs/nestjs/winston/*.md"
        ],
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "main": "libs/nestjs/winston/src/index.ts",
        "outputPath": "dist/libs/nestjs/winston",
        "packageJson": "libs/nestjs/winston/package.json",
        "tsConfig": "libs/nestjs/winston/tsconfig.lib.json",
        "updateBuildableProjectDepsInPackageJson": true
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/nestjs/winston/**/*.ts"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/nestjs/winston/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": [
        "coverage/libs/nestjs/winston"
      ]
    }
  }
}
