{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "generators-graphql-resolvers",
  "projectType": "library",
  "sourceRoot": "libs/generators/graphql-resolvers/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "assets": ["libs/generators/graphql-resolvers/*.md"],
        "main": "libs/generators/graphql-resolvers/src/index.ts",
        "outputPath": "dist/libs/generators/graphql-resolvers",
        "packageJson": "libs/generators/graphql-resolvers/package.json",
        "tsConfig": "libs/generators/graphql-resolvers/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/generators/graphql-resolvers/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "publish": {},
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generators/graphql-resolvers/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generators/graphql-resolvers"]
    }
  }
}
