{
  "prefix": "tractr",
  "projectType": "library",
  "sourceRoot": "libs/angular/models/features/src",
  "tags": ["angular"],
  "targets": {
    "build": {
      "configurations": {
        "development": {
          "tsConfig": "libs/angular/models/features/tsconfig.lib.json"
        },
        "production": {
          "tsConfig": "libs/angular/models/features/tsconfig.lib.prod.json"
        }
      },
      "defaultConfiguration": "production",
      "executor": "@nrwl/angular:ng-packagr-lite",
      "options": {
        "project": "libs/angular/models/features/ng-package.json"
      },
      "outputs": ["dist/libs/angular/models/features"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/angular/models/features/**/*.ts",
          "libs/angular/models/features/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/angular/models/features/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/angular/models/features"]
    }
  }
}
