{
  "projectType": "library",
  "root": "libs/generated/prisma",
  "sourceRoot": "libs/generated/prisma/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "options": {
        "assets": ["libs/generated/prisma/*.md"],
        "main": "libs/generated/prisma/src/index.ts",
        "outputPath": "dist/libs/generated/prisma",
        "packageJson": "libs/generated/prisma/package.json",
        "tsConfig": "libs/generated/prisma/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/generated/prisma/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generated/prisma/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generated/prisma"]
    }
  }
}
