{
  "implicitDependencies": [
    "hapify-templates-nestjs-models-common",
    "nestjs-core",
    "nestjs-database",
    "generated-models"
  ],
  "projectType": "library",
  "root": "libs/generated/nestjs-models-common",
  "sourceRoot": "libs/generated/nestjs-models-common/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "options": {
        "assets": ["libs/generated/nestjs-models-common/*.md"],
        "main": "libs/generated/nestjs-models-common/src/index.ts",
        "outputPath": "dist/libs/generated/nestjs-models-common",
        "packageJson": "libs/generated/nestjs-models-common/package.json",
        "tsConfig": "libs/generated/nestjs-models-common/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "generate": {
      "executor": "./dist/libs/schematics:generate",
      "options": {
        "cwd": "libs/generated/nestjs-models-common"
      },
      "outputs": [
        "libs/generated/nestjs-models-common/src/generated",
        "libs/generated/nestjs-models-common/mock/src/generated"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/generated/nestjs-models-common/**/*.ts",
          "libs/generated/nestjs-models-common/mock/**/*.ts",
          "libs/generated/nestjs-models-common/mock/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generated/nestjs-models-common/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generated/nestjs-models-common"]
    }
  }
}
