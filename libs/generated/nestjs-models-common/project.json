{
  "implicitDependencies": [
    "hapify-templates-nestjs-models-common",
    "nestjs-core",
    "nestjs-database",
    "generated-models"
  ],
  "projectType": "library",
  "root": "libs/generated/nestjs-models-common",
  "sourceRoot": "libs/generated/nestjs-models-common/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "options": {
        "assets": ["libs/generated/nestjs-models-common/*.md"],
        "main": "libs/generated/nestjs-models-common/src/index.ts",
        "outputPath": "dist/libs/generated/nestjs-models-common",
        "packageJson": "libs/generated/nestjs-models-common/package.json",
        "tsConfig": "libs/generated/nestjs-models-common/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "generate": {
      "dependsOn": [
        {
          "projects": "self",
          "target": "pregenerate"
        }
      ],
      "executor": "./dist/libs/schematics:generate",
      "options": {
        "cwd": "libs/generated/nestjs-models-common"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/generated/nestjs-models-common/**/*.ts",
          "libs/generated/nestjs-models-common/mock/**/*.ts",
          "libs/generated/nestjs-models-common/mock/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "pregenerate": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "rm -rf node_modules/@tractr/{hapify-config,hapify-generate-config,hapify-common,hapify-update-templates-import-path,hapify-templates-nestjs-models-common}",
            "description": "Delete previous Hapify Common from node modules",
            "forwardAllArgs": false
          },
          {
            "command": "mkdir -p node_modules/@tractr",
            "description": "Ensure the directory exists before the copy",
            "forwardAllArgs": false
          },
          {
            "command": "cp -rf dist/libs/config/hapify/ node_modules/@tractr/hapify-config/",
            "description": "Copy the current build to node_modules",
            "forwardAllArgs": false
          },
          {
            "command": "cp -rf dist/libs/hapify/generate-config/ node_modules/@tractr/hapify-generate-config/",
            "description": "Copy the current build to node_modules",
            "forwardAllArgs": false
          },
          {
            "command": "cp -rf dist/libs/hapify/hapify-common/ node_modules/@tractr/hapify-common/",
            "description": "Copy the current build to node_modules",
            "forwardAllArgs": false
          },
          {
            "command": "cp -rf dist/libs/hapify/update-templates-import-path/ node_modules/@tractr/update-templates-import-path/",
            "description": "Copy the current build to node_modules",
            "forwardAllArgs": false
          },
          {
            "command": "cp -rf dist/libs/hapify/templates/nestjs-models-common node_modules/@tractr/hapify-templates-nestjs-models-common/",
            "description": "Copy the current build to node_modules",
            "forwardAllArgs": false
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generated/nestjs-models-common/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generated/nestjs-models-common"]
    }
  }
}
