{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["hapify-templates-dbml"],
  "projectType": "library",
  "sourceRoot": "libs/generated/dbml/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "assets": ["libs/generated/dbml/*.md"],
        "main": "libs/generated/dbml/src/index.ts",
        "outputPath": "dist/libs/generated/dbml",
        "packageJson": "libs/generated/dbml/package.json",
        "tsConfig": "libs/generated/dbml/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "generate": {
      "executor": "./dist/libs/schematics:generate",
      "options": {
        "cwd": "libs/generated/dbml",
        "format": false,
        "moveGeneratedFiles": false,
        "updateImportPath": false
      },
      "outputs": ["libs/generated/dbml/models.dbml"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/generated/dbml/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generated/dbml/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generated/dbml"]
    }
  }
}
