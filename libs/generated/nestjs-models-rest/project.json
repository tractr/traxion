{
  "implicitDependencies": [
    "hapify-templates-nestjs-models-rest",
    "nestjs-core",
    "nestjs-database",
    "nestjs-mailer",
    "nestjs-authentication",
    "nestjs-casl",
    "generated-casl",
    "generated-models",
    "generated-rest-dtos",
    "generated-nestjs-models-common"
  ],
  "projectType": "library",
  "root": "libs/generated/nestjs-models-rest",
  "sourceRoot": "libs/generated/nestjs-models-rest/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "options": {
        "assets": ["libs/generated/nestjs-models-rest/*.md"],
        "main": "libs/generated/nestjs-models-rest/src/index.ts",
        "outputPath": "dist/libs/generated/nestjs-models-rest",
        "packageJson": "libs/generated/nestjs-models-rest/package.json",
        "tsConfig": "libs/generated/nestjs-models-rest/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "generate": {
      "executor": "./dist/libs/schematics:generate",
      "options": {
        "cwd": "libs/generated/nestjs-models-rest"
      },
      "outputs": [
        "libs/generated/nestjs-models-rest/src/generated",
        "libs/generated/nestjs-models-rest/mock/src/generated"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/generated/nestjs-models-rest/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generated/nestjs-models-rest/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generated/nestjs-models-rest"]
    }
  }
}
