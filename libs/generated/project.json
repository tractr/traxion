{
  "implicitDependencies": [
    "common",
    "hapify-templates-models",
    "hapify-templates-rest-dtos",
    "hapify-templates-nestjs-models",
    "hapify-templates-nestjs-models-common",
    "hapify-templates-nestjs-models-rest",
    "hapify-templates-rext-client",
    "hapify-templates-angular-rext-client",
    "nestjs-core",
    "nestjs-database",
    "nestjs-authentication",
    "nestjs-casl",
    "nestjs-testing",
    "angular-tools",
    "hapify-config",
    "hapify-common",
    "hapify-generate-config",
    "hapify-update-templates-import-path",
    "schematics"
  ],
  "projectType": "library",
  "root": "libs/generated",
  "sourceRoot": "libs/generated/src",
  "tags": ["scope:generate"],
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "options": {
        "assets": ["libs/generated/*.md"],
        "main": "libs/generated/src/index.ts",
        "outputPath": "dist/libs/generated",
        "packageJson": "libs/generated/package.json",
        "tsConfig": "libs/generated/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "generate": {
      "dependsOn": [
        {
          "projects": "dependencies",
          "target": "pregenerate"
        }
      ],
      "executor": "./dist/libs/schematics:generate",
      "options": {
        "cwd": "libs/generated",
        "inputHapifyGeneratedPath": "generated",
        "outputGeneratedPath": "generated"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generated/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generated"]
    }
  }
}
