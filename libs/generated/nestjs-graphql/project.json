{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": [
    "common",
    "nestjs-graphql",
    "hapify-templates-nestjs-graphql",
    "generated-casl",
    "generated-nestjs-models-common",
    "hapify-generators-graphql-resolvers"
  ],
  "prefix": "tractr",
  "projectType": "library",
  "sourceRoot": "libs/generated/nestjs-graphql/src",
  "tags": [],
  "targets": {
    "generate": {
      "executor": "./dist/libs/schematics:generate",
      "options": {
        "cwd": "libs/generated/nestjs-graphql"
      },
      "outputs": ["libs/generated/nestjs-graphql/src/generated"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/generated/nestjs-graphql/src/**/*.ts",
          "libs/generated/nestjs-graphql/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/generated/nestjs-graphql/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/generated/nestjs-graphql"]
    },
    "ts-morph-generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx ts-node -r tsconfig-paths/register --project ./tsconfig.lib.json ./src/generate.ts",
            "forwardAllArgs": false
          }
        ],
        "cwd": "libs/generated/nestjs-graphql",
        "parallel": false
      }
    }
  }
}
