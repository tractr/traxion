{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "affected": {
    "defaultBase": "main"
  },
  "generators": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "style": "less",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "less"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "style": "less",
      "unitTestRunner": "jest"
    },
    "@nrwl/react": {
      "application": {
        "babel": true,
        "linter": "eslint",
        "style": "less"
      },
      "component": {
        "style": "less"
      },
      "library": {
        "linter": "eslint",
        "style": "less"
      }
    }
  },
  "implicitDependencies": {
    ".eslintrc.json": "*",
    "hapify-models.json": "*",
    "nx.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "workspace.json": "*"
  },
  "npmScope": "tractr",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "publish": {
      "dependsOn": ["^publish"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "accessToken": "ZDY0OTk5NzAtZTQzMy00ZDU5LWI4ZjYtZDI0NDgwNmFiNmE0fHJlYWQ=",
        "cacheableOperations": ["build", "test", "lint", "e2e"],
        "parallel": 1
      },
      "runner": "@nrwl/nx-cloud"
    }
  },
  "useDaemonProcess": true
}
