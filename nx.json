{
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    ".eslintrc.json": "*",
    "hapify-models.json": "*",
    "nx.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "workspace.json": "*"
  },
  "npmScope": "tractr",
  "projects": {
    "angular-authentication": {
      "implicitDependencies": [
        "hapify-templates-models",
        "hapify-templates-prisma",
        "hapify-config",
        "hapify-generate-config"
      ],
      "tags": ["scope:angular"]
    },
    "angular-casl": {
      "tags": []
    },
    "angular-components": {
      "tags": ["scope:angular"]
    },
    "angular-config": {
      "tags": []
    },
    "angular-file-storage": {
      "tags": []
    },
    "angular-form": {
      "tags": ["scope:angular"]
    },
    "angular-tools": {
      "tags": ["scope:angular"]
    },
    "api": {
      "tags": ["scope:app"]
    },
    "commit-config": {
      "tags": ["scope:config"]
    },
    "common": {
      "tags": ["scope:common"]
    },
    "eslint-config": {
      "tags": ["scope:config"]
    },
    "generated": {
      "implicitDependencies": [
        "common",
        "hapify-templates-models",
        "hapify-templates-rest-dtos",
        "hapify-templates-nestjs-models",
        "hapify-templates-nestjs-models-common",
        "hapify-templates-nestjs-models-rest",
        "hapify-templates-rext-client",
        "hapify-templates-angular-rext-client",
        "nestjs-core",
        "nestjs-database",
        "nestjs-testing",
        "angular-tools",
        "hapify-config",
        "hapify-generate-config"
      ],
      "tags": ["scope:generate"]
    },
    "hapify-config": {
      "tags": ["scope:config"]
    },
    "hapify-generate-config": {
      "tags": ["scope:config"]
    },
    "hapify-templates-angular-rext-client": {
      "tags": ["scope:template"]
    },
    "hapify-templates-casl": {
      "tags": ["scope:template"]
    },
    "hapify-templates-dbml": {
      "tags": ["scope:template"]
    },
    "hapify-templates-models": {
      "tags": ["scope:template"]
    },
    "hapify-templates-nestjs-models": {
      "tags": ["scope:template"]
    },
    "hapify-templates-nestjs-models-common": {
      "tags": ["scope:template"]
    },
    "hapify-templates-nestjs-models-rest": {
      "tags": ["scope:template"]
    },
    "hapify-templates-prisma": {
      "tags": ["scope:template"]
