    "hapify-templates-rest-dtos": {
      "tags": ["scope:template"]
    },
    "hapify-templates-rext-client": {
      "tags": ["scope:template"]
    },
    "nestjs-authentication": {
      "implicitDependencies": [
        "hapify-templates-models",
        "hapify-templates-prisma",
        "hapify-templates-nestjs-models-common",
        "nestjs-core",
        "nestjs-database",
        "hapify-config",
        "hapify-generate-config"
      ],
      "tags": ["scope:nestjs"]
    },
    "nestjs-casl": {
      "implicitDependencies": ["hapify-config", "hapify-generate-config"],
      "tags": ["scope:nestjs"]
    },
    "nestjs-core": {
      "tags": ["scope:nestjs"]
    },
    "nestjs-database": {
      "tags": ["scope:nestjs"]
    },
    "nestjs-file-storage": {
      "tags": ["scope:nestjs"]
    },
    "nestjs-mailer": {
      "tags": ["scope:nestjs"]
    },
    "nestjs-testing": {
      "tags": ["scope:nestjs"]
    },
    "prettier-config": {
      "tags": ["scope:config"]
    },
    "prisma": {
      "implicitDependencies": [
        "hapify-templates-prisma",
        "hapify-config",
        "generated",
        "hapify-generate-config"
      ],
      "tags": ["scope:generated"]
    },
    "pwa": {
      "tags": ["scope:app"]
    },
    "pwa-e2e": {
      "implicitDependencies": ["pwa"],
      "tags": ["scope:app"]
    },
    "terraform-api-service": {
      "tags": []
    },
    "terraform-aws-component": {
      "tags": []
    },
    "terraform-aws-stack": {
      "tags": []
    },
    "terraform-network-group": {
      "tags": []
    },
    "terraform-pool-group": {
      "tags": []
    },
    "terraform-pwa-service": {
      "tags": []
    },
    "terraform-registry-group": {
      "tags": []
    },
    "terraform-s3-component": {
      "tags": []
    },
    "terraform-s3-user-component": {
      "tags": []
    },
    "terraform-zone-group": {
      "tags": []
    }
  },
  "targetDependencies": {
    "build": [
      {
        "projects": "dependencies",
        "target": "build"
      }
    ],
    "generate": [
      {
        "projects": "dependencies",
        "target": "pregenerate"
      }
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      },
      "runner": "@nrwl/workspace/tasks-runners/default"
    }
  }
}
