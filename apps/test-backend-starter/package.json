{
  "name": "test-backend-starter",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "generate": "npm run hpf:generate && npm run prisma:merge-schema && npm run prisma:format && npm run prisma:generate",
    "hpf:generate": "npm run hpf:generate:config && hpf generate",
    "hpf:generate:config": "hpf-generate-config",
    "lint": "npm run lint:tsc && npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint \"{src,test}/**/*.ts\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "npm run lint:tsc:fix && npm run lint:prettier:fix && npm run lint:eslint:fix",
    "lint:prettier": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:prettier:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:fix": "npm run lint:tsc",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:merge-schema": "prisma-merge --baseFile=prisma/schemas/base.prisma --schemaFilePatterns=prisma/**/*.prisma --outputFile=prisma/schema.prisma --excludedFilePattern=prisma/**/schema.prisma",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:reset": "prisma reset",
    "start": "nest start",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --config ./test/jest-unit-test.json",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/common": "^7.6.11",
    "@nestjs/core": "^7.6.11",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/platform-express": "^7.6.11",
    "@nestjs/swagger": "^4.7.12",
    "@prisma/client": "^2.17.0",
    "@tractr/hapify-plugin-nestjs-core": "^0.0.0",
    "@tractr/hapify-plugin-nestjs-database": "^0.0.0",
    "@tractr/hapify-plugin-nestjs-user": "^0.0.0",
    "@tractr/hapify-templates-nestjs-models": "^0.0.0",
    "@tractr/hapify-templates-prisma": "^0.0.0",
    "apollo-server-express": "^2.19.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@hapify/cli": "^0.9.0",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.11",
    "@tractr/eslint-config": "^1.2.5",
    "@tractr/hapify-generate-config": "^0.0.1",
    "@tractr/hapify-templates-nestjs-models": "^0.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "prettier": "^2.2.1",
    "prisma": "^2.17.0",
    "prisma-merge": "^0.2.0",
    "semantic-release": "^17.3.7",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  }
}
