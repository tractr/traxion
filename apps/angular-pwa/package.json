{
  "name": "angular-pwa",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "generate": "rimraf generated && npm run hpf:generate && npm run lint:prettier:fix",
    "hpf:generate": "npm run hpf:generate:config && hpf generate",
    "hpf:serve": "hpf serve",
    "hpf:generate:config": "hpf-generate-config",
    "build": "ng build",
    "lint": "npm run lint:tsc && npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint \"{src,e2e}/**/*.{ts,js}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "npm run lint:tsc:fix && npm run lint:prettier:fix && npm run lint:eslint:fix",
    "lint:prettier": "prettier --check \"{src,e2e}/**/*.{ts,js}\"",
    "lint:prettier:fix": "prettier --write \"{src,e2e}/**/*.{ts,js}\"",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:fix": "npm run lint:tsc",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@angular/service-worker": "~11.2.3",
    "@nguniversal/common": "^11.1.2",
    "@ngx-translate/core": "^13.0.0",
    "@tractr/angular-tools": "1.1.0",
    "bootstrap": "^4.4.1",
    "ng-zorro-antd": "^11.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@nguniversal/builders": "^11.1.2",
    "@tractr/hapify-config": "^1.0.0",
    "@tractr/hapify-templates-models": "^1.2.2",
    "@tractr/hapify-templates-rest-dtos": "^1.2.2",
    "@tractr/hapify-templates-rext-client": "^1.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  }
}
