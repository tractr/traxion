{
  "name": "angular-pwa",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "prebuild": "ngcc",
    "build": "ng build",
    "build-storybook": "yarn docs:json && build-storybook",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "e2e": "ng e2e",
    "generate": "rimraf generated && npm run hpf:generate && npm run lint:prettier:fix",
    "hpf:generate": "npm run hpf:generate:config && hpf generate",
    "hpf:generate:config": "hpf-generate-config",
    "hpf:serve": "hpf serve",
    "lint": "npm run lint:tsc && npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint \"{src,e2e}/**/*.{ts,js}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "npm run lint:tsc:fix && npm run lint:prettier:fix && npm run lint:eslint:fix",
    "lint:prettier": "prettier --check \"{src,e2e}/**/*.{ts,js}\"",
    "lint:prettier:fix": "prettier --write \"{src,e2e}/**/*.{ts,js}\"",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:fix": "npm run lint:tsc",
    "ng": "ng",
    "start": "ng serve",
    "storybook": "yarn docs:json && start-storybook -p 6006"
  },
  "dependencies": {
    "@angular/animations": "~11.2.12",
    "@angular/common": "~11.2.12",
    "@angular/compiler": "~11.2.12",
    "@angular/core": "~11.2.12",
    "@angular/forms": "~11.2.12",
    "@angular/platform-browser": "~11.2.12",
    "@angular/platform-browser-dynamic": "~11.2.12",
    "@angular/router": "~11.2.12",
    "@angular/service-worker": "~11.2.12",
    "@ngneat/tailwind": "^7.0.3",
    "@nguniversal/common": "^11.2.1",
    "@ngx-translate/core": "^13.0.0",
    "@tractr/angular-components": "1.0.0",
    "@tractr/angular-tools": "1.1.0",
    "ng-zorro-antd": "^11.4.1",
    "rxjs": "^6.6.7",
    "universal-rxjs-ajax": "^2.0.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.11",
    "@angular/cli": "~11.2.11",
    "@angular/compiler-cli": "~11.2.12",
    "@babel/core": "^7.14.2",
    "@compodoc/compodoc": "^1.1.11",
    "@nguniversal/builders": "^11.2.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/angular": "^6.2.9",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.3.2",
    "@tailwindcss/line-clamp": "0.2.0",
    "@tailwindcss/typography": "0.4.0",
    "@tractr/hapify-config": "^1.0.0",
    "@tractr/hapify-templates-models": "^1.2.3",
    "@tractr/hapify-templates-rest-dtos": "^1.2.3",
    "@tractr/hapify-templates-rext-client": "^1.0.3",
    "@types/jasmine": "~3.6.11",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.8.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "tailwindcss": "2.1.2",
    "typescript": "4.1.5"
  }
}
