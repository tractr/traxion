{
  "name": "backend-starter",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/tractr/stack"
  },
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run generate && echo \"\nBuilding nest js app\" && nest build && echo \"Done\"",
    "generate": "rimraf generated && npm run hpf:generate && npm run prisma:merge-schema && npm run prisma:format && npm run prisma:generate",
    "hpf:generate": "npm run hpf:generate:config && hpf generate",
    "hpf:generate:config": "hpf-generate-config",
    "hpf:serve": "hpf serve",
    "lint": "npm run lint:tsc && npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint \"{src,test}/**/*.ts\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "npm run lint:tsc:fix && npm run lint:prettier:fix && npm run lint:eslint:fix",
    "lint:prettier": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"generated/**/*.ts\"",
    "lint:prettier:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"generated/**/*.ts\"",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:fix": "npm run lint:tsc",
    "prisma": "prisma",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:merge-schema": "prisma-merge --baseFile=prisma/schemas/base.prisma --schemaFilePatterns=prisma/**/*.prisma --outputFile=prisma/schema.prisma --excludedFilePattern=prisma/**/schema.prisma",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:push": "prisma db push --preview-feature",
    "prisma:reset": "prisma reset",
    "prisma:seed": "prisma db seed --preview-feature",
    "start": "nest start",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:prod": "node dist/src/main",
    "test": "jest --config ./test/jest-unit-test.json",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "@prisma/client": "^2.22.1",
    "@tractr/nestjs-authentication": "1.8.2",
    "@tractr/nestjs-core": "1.5.0",
    "@tractr/nestjs-database": "1.3.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "9.0.2",
    "dotenv-expand": "5.1.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "prisma": "^2.22.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.15",
    "@tractr/hapify-config": "1.0.0",
    "@tractr/hapify-templates-models": "1.2.3",
    "@tractr/hapify-templates-nestjs-models": "1.3.3",
    "@tractr/hapify-templates-nestjs-models-common": "1.2.5",
    "@tractr/hapify-templates-nestjs-models-rest": "1.3.3",
    "@tractr/hapify-templates-prisma": "1.2.1",
    "@tractr/hapify-templates-rest-dtos": "1.2.3",
    "@tractr/nestjs-testing": "1.2.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.5",
    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "faker": "^5.5.3",
    "jest-mock-extended": "^1.0.14",
    "pg": "^8.6.0",
    "prisma-merge": "^0.2.0",
    "supertest": "^6.1.3"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Changelog\n\nAll notable changes to this project will be documented in this file. See\n[Conventional Commits](https://conventionalcommits.org) for commit guidelines."
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
