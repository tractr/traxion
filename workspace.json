{
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "pwa",
  "generators": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "style": "less",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "less"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "style": "less",
      "unitTestRunner": "jest"
    }
  },
  "projects": {
    "angular-components": {
      "prefix": "tractr",
      "projectType": "library",
      "root": "libs/angular/components",
      "sourceRoot": "libs/angular/components/src",
      "targets": {
        "build": {
          "configurations": {
            "development": {
              "tsConfig": "libs/angular/components/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "libs/angular/components/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production",
          "executor": "@nrwl/angular:package",
          "options": {
            "project": "libs/angular/components/ng-package.json"
          },
          "outputs": ["dist/libs/angular/components"]
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular/components/src/**/*.ts",
              "libs/angular/components/src/**/*.html"
            ]
          }
        },
        "publish": {
          "executor": "ngx-deploy-npm:deploy",
          "options": {
            "access": "public",
            "buildTarget": "production"
          }
        },
        "release": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/angular/components/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/angular/components"]
        }
      }
    },
    "angular-tools": {
      "prefix": "stack",
      "projectType": "library",
      "root": "libs/angular/tools",
      "sourceRoot": "libs/angular/tools/src",
      "targets": {
        "build": {
          "configurations": {
            "development": {
              "tsConfig": "libs/angular/tools/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "libs/angular/tools/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production",
          "executor": "@nrwl/angular:package",
          "options": {
            "project": "libs/angular/tools/ng-package.json"
          },
          "outputs": ["dist/libs/angular/tools"]
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular/tools/src/**/*.ts",
              "libs/angular/tools/src/**/*.html"
            ]
          }
        },
        "publish": {
          "executor": "ngx-deploy-npm:deploy",
          "options": {
            "access": "public",
            "buildTarget": "production"
          }
        },
        "release": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/angular/tools/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/angular/tools"]
        }
      }
    },
    "api": {
      "projectType": "application",
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "targets": {
        "cli": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["ts-node -r tsconfig-paths/register src/cli.ts"],
            "cwd": "apps/api",
            "parallel": false
          }
        },
        "build": {
          "configurations": {
            "production": {
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ],
              "inspect": false,
              "optimization": true
            }
          },
          "executor": "@nrwl/node:build",
          "options": {
            "assets": ["apps/api/src/assets"],
            "main": "apps/api/src/main.ts",
            "outputPath": "dist/apps/api",
            "tsConfig": "apps/api/tsconfig.app.json"
          },
          "outputs": ["{options.outputPath}"]
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "release": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build",
            "inspect": true,
            "port": 7777
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/api"]
        }
      }
    },
    "api-interfaces": {
      "projectType": "library",
      "root": "libs/api-interfaces",
